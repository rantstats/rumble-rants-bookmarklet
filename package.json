{
    "name": "rumble-rants-bookmarklet",
    "version": "1.3.1",
    "description": "Show and keep rants in Rumble",
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc",
        "build-watch": "yarn run build --watch",
